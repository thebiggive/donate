{
  "name": "donate-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "test-ci": "ng test --karma-config ./karma-ci.conf.js --no-watch --no-progress --browsers=ChromeHeadlessNoSandbox",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server:production": "webpack --config webpack.server.production.config.js --progress --colors",
    "compile:server:regression": "webpack --config webpack.server.regression.config.js --progress --colors",
    "compile:server:staging": "webpack --config webpack.server.staging.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build": "ng build --deploy-url=/d/",
    "build:staging": "ng build --deploy-url=/d/ --prod --configuration=staging",
    "build:regression": "ng build --deploy-url=/d/ --prod --configuration=regression",
    "build:production": "ng build --deploy-url=/d/ --prod",
    "build:ssr:production": "npm run build:client-and-server-bundles:production && npm run compile:server:production",
    "build:ssr:regression": "npm run build:client-and-server-bundles:regression && npm run compile:server:regression",
    "build:ssr:staging": "npm run build:client-and-server-bundles:staging && npm run compile:server:staging",
    "build:client-and-server-bundles:production": "npm run build:production && ng run donate-frontend:server:production",
    "build:client-and-server-bundles:regression": "npm run build:regression && ng run donate-frontend:server:regression",
    "build:client-and-server-bundles:staging": "npm run build:staging && ng run donate-frontend:server:staging",
    "e2e-ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "ci": "npm run lint && npm run test-ci && npm run e2e-ci"
  },
  "dependencies": {
    "@angular/animations": "^8.2.13",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.13",
    "@angular/compiler": "^8.2.13",
    "@angular/core": "^8.2.13",
    "@angular/forms": "^8.2.13",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.13",
    "@angular/platform-browser-dynamic": "^8.2.13",
    "@angular/platform-server": "^8.2.13",
    "@angular/router": "^8.2.13",
    "@ngmodule/material-carousel": "^0.5.1",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.1.1",
    "bootstrap": "^4.3.1",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "hammerjs": "^2.0.8",
    "helmet": "^3.21.2",
    "morgan": "^1.9.1",
    "ngx-webstorage-service": "^4.1.0",
    "rxjs": "^6.5.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.802.2",
    "@angular/cli": "^8.3.17",
    "@angular/compiler-cli": "^8.2.13",
    "@angular/language-service": "^8.2.13",
    "@types/jasmine": "^3.4.6",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.2.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "puppeteer": "^1.20.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "webpack-cli": "^3.3.10"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  }
}
